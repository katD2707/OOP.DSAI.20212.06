<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane prefHeight="500.0" prefWidth="900.0" style="-fx-background-color: gray;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
   <center>
      <AnchorPane prefHeight="500.0" prefWidth="700.0" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER_LEFT">
               <children>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="160.0" style="-fx-background-color: #bbb;">
                     <children>
                        <Button fx:id="btnInVol" alignment="CENTER" mnemonicParsing="false" onAction="#btnVolAct" onKeyPressed="#btnVolPressed" prefHeight="40.0" prefWidth="40.0" text="+">
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin>
                        </Button>
                        <Label fx:id="lblVol" text="Volume: 50">
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin>
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                        </Label>
                        <Button fx:id="btnDeVol" mnemonicParsing="false" onAction="#btnVolAct" onKeyPressed="#btnVolPressed" prefHeight="40.0" prefWidth="40.0" text="-">
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                     <HBox.margin>
                        <Insets left="20.0" right="20.0" />
                     </HBox.margin>
                  </VBox>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="160.0" style="-fx-background-color: #aaaaaa;" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets right="20.0" />
                     </HBox.margin>
                     <children>
                        <Button fx:id="btnInOct" mnemonicParsing="false" onAction="#btnOctAct" onKeyPressed="#btnOctPressed" prefHeight="40.0" prefWidth="40.0" text="+">
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin>
                        </Button>
                        <Label fx:id="lblOct" text="Octave: 5">
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin>
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                        </Label>
                        <Button fx:id="btnDeOct" mnemonicParsing="false" onAction="#btnOctAct" onKeyPressed="#btnOctPressed" prefHeight="40.0" prefWidth="40.0" text="-">
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <Button fx:id="btnC" alignment="BOTTOM_CENTER" layoutY="200.0" mnemonicParsing="false" onAction="#noteAct" onKeyPressed="#notePressed" onKeyReleased="#noteReleased" prefHeight="300.0" prefWidth="100.0" text="C" />
            <Button fx:id="noteD" alignment="BOTTOM_CENTER" layoutX="100.0" layoutY="200.0" mnemonicParsing="false" onAction="#noteAct" onKeyPressed="#notePressed" onKeyReleased="#noteReleased" prefHeight="300.0" prefWidth="100.0" text="D" />
            <Button fx:id="noteE" alignment="BOTTOM_CENTER" layoutX="200.0" layoutY="200.0" mnemonicParsing="false" onAction="#noteAct" onKeyPressed="#notePressed" onKeyReleased="#noteReleased" prefHeight="300.0" prefWidth="100.0" text="E" />
            <Button fx:id="noteF" alignment="BOTTOM_CENTER" layoutX="300.0" layoutY="200.0" mnemonicParsing="false" onAction="#noteAct" onKeyPressed="#notePressed" onKeyReleased="#noteReleased" prefHeight="300.0" prefWidth="100.0" text="F" />
            <Button fx:id="btnCd" alignment="BOTTOM_CENTER" layoutX="65.0" layoutY="200.0" mnemonicParsing="false" onAction="#noteAct" onKeyPressed="#notePressed" onKeyReleased="#noteReleased" prefHeight="250.0" prefWidth="70.0" style="-fx-background-color: black; -fx-text-fill: white;" text="C#" textFill="TRANSPARENT">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Button>
            <Button fx:id="noteG" alignment="BOTTOM_CENTER" layoutX="400.0" layoutY="200.0" mnemonicParsing="false" onAction="#noteAct" onKeyPressed="#notePressed" onKeyReleased="#noteReleased" prefHeight="300.0" prefWidth="100.0" text="G" />
            <Button fx:id="noteA" alignment="BOTTOM_CENTER" layoutX="500.0" layoutY="200.0" mnemonicParsing="false" onAction="#noteAct" onKeyPressed="#notePressed" onKeyReleased="#noteReleased" prefHeight="300.0" prefWidth="100.0" text="A" />
            <Button fx:id="noteB" alignment="BOTTOM_CENTER" layoutX="600.0" layoutY="200.0" mnemonicParsing="false" onAction="#noteAct" onKeyPressed="#notePressed" onKeyReleased="#noteReleased" prefHeight="300.0" prefWidth="100.0" text="B" />
            <Button fx:id="noteDe" alignment="BOTTOM_CENTER" layoutX="165.0" layoutY="200.0" mnemonicParsing="false" onAction="#noteAct" onKeyPressed="#notePressed" onKeyReleased="#noteReleased" prefHeight="250.0" prefWidth="70.0" style="-fx-background-color: black; -fx-text-fill: white;" text="D#" textFill="TRANSPARENT">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Button>
            <Button fx:id="noteFg" alignment="BOTTOM_CENTER" layoutX="365.0" layoutY="200.0" mnemonicParsing="false" onAction="#noteAct" onKeyPressed="#notePressed" onKeyReleased="#noteReleased" prefHeight="250.0" prefWidth="70.0" style="-fx-background-color: black; -fx-text-fill: white;" text="F#" textFill="TRANSPARENT">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Button>
            <Button fx:id="noteGa" alignment="BOTTOM_CENTER" layoutX="465.0" layoutY="200.0" mnemonicParsing="false" onAction="#noteAct" onKeyPressed="#notePressed" onKeyReleased="#noteReleased" prefHeight="250.0" prefWidth="70.0" style="-fx-background-color: black; -fx-text-fill: white;" text="G#" textFill="TRANSPARENT">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Button>
            <Button fx:id="noteAb" alignment="BOTTOM_CENTER" layoutX="565.0" layoutY="200.0" mnemonicParsing="false" onAction="#noteAct" onKeyPressed="#notePressed" onKeyReleased="#noteReleased" prefHeight="250.0" prefWidth="70.0" style="-fx-background-color: black; -fx-text-fill: white;" text="A#" textFill="TRANSPARENT">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </center>
   <left>
      <Region prefHeight="200.0" prefWidth="150.0" style="-fx-background-color: brown;" BorderPane.alignment="CENTER" />
   </left>
   <right>
      <Region prefHeight="200.0" prefWidth="150.0" style="-fx-background-color: brown;" BorderPane.alignment="CENTER" />
   </right>
</BorderPane>
